class Solution {
public:
    vector<int> createTargetArray(vector<int>& nums, vector<int>& index) {
        int n = nums.size();

        vector<int> target;

        for(int i=0;i<n;i++){

            int sex = nums[i];

            int fck = index[i];

       target.insert(target.begin()+fck,sex);
            
        }
return target;
    }
};
